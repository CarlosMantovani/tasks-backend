pipeline {
    agent any
    stages {
        stage ('Build Backend') {
            steps {
                bat 'mvn clean package -DskipTests=true'
            }
        }
        stage ('meio') {
            steps {
                bat 'echo meio'
            }
        }
        stage ('fim') {
            steps {
                sleep(5)
                bat 'echo fim'
            }
        }
    }
}